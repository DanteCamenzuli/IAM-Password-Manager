<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<resource-bundle>
			<base-name>com.doubleclue.dcem.oauth.resources.Messages</base-name>
			<var>oauthMsg</var>
		</resource-bundle>
	</application>
	
	<navigation-rule>
		<display-name>OAUTH_LOGIN</display-name>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>oauthLogin</from-outcome>
			<to-view-id>/oauth/login.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
</faces-config>
